<div class="cover">
  <div>
    <h1>Have an amazing dining experience...</h1>
    <h2><span data-controller="typed-js"></span></h2>
  </div>
  <br>
  <div>
  <a class="btn button-dark"<%= link_to "Find an experience", dinings_path %></a>
  </div >
</div>

<div class="container p-5">

  <%= form_tag dinings_path, method: 'get', :class => 'input-group' do  %>
        <input type="search" class="form-control rounded" placeholder="Where would you like to dine?" <%= text_field_tag :query,params[:query] %>
        <%= submit_tag "Search", class: 'btn btn-outline-primary' %>
    </div>
  <%end%>

</div>
