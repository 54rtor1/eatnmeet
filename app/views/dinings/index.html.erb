<div class="cover mb-4 d-flex align-items-center justify-content-center">
  <div class="text-center">
    <h1>All dining experiences</h1>
    <h3 class="typed-js" data-controller="typed-js"></h3>
  </div>
</div>
<div class="container py-5">

  <div class="row">
    <ul>
      <% @dinings.each do |dining| %>
        <li>
          <%= link_to dining.name, dining_path(dining.id) %>
        </li>
      <% end %>
    </ul>
  </div>
  <div class="d-flex align-items-center justify-content-between mb-3">
    <%= link_to "Create dining", new_dining_path, class: "btn btn-primary" %>
  </div>

</div>

  <div style="width: 100%; height: 600px;"
  data-controller="mapbox"
  data-mapbox-markers-value="<%= @markers.to_json %>"
  data-mapbox-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>"></div>
  </div>
